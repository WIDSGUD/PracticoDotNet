@model Pais

<h1>ELiminar Pais</h1>
<form method="post">
    <input asp-for="id" hidden/>
    <div>
        <div>
            <label>Nuevo nombre</label>
            <input asp-for="nombre" disabled type="text" class="form-control" />
        </div>
        <button type="submit" class="btn btn-danger">Eliminar</button>
    </div>
</form>